{
    "comments": {
        "blockComment": [ "/*", "*/" ],
        "lineComment": "//"
    },
    "brackets": [
        [ "(", ")" ],
        [ "[", "]" ],
        [ "{", "}" ]
    ],
    "autoClosingPairs": [
        { "open": "(", "close": ")" },
        { "open": "[", "close": "]" },
        { "open": "{", "close": "}" },
        { "open": "'", "close": "'",
            "notIn": [ "comment", "string" ] },
        { "open": "\"", "close": "\"",
            "notIn": [ "string" ]},
        { "open": "/*", "close": "*/",
            "notIn": [ "string" ]}
    ],
    "surroundingPairs": [
        [ "(", ")" ],
        [ "[", "]" ],
        [ "{", "}" ],
        [ "\"", "\"" ],
        [ "'", "'" ]
    ]
}