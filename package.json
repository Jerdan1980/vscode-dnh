{
    "name": "vscode-dnh",
    "displayName": "Touhou Danmakufu ph3 Support",
    "version": "0.0.1",
    "description": "Snippets, Intellisense, and Colouring for Touhou Danmakufu language",
    "publisher": "Jerdan1980",
    "repository": {
        "type": "git",
        "url": "https://github.com/Jerdan1980/vscode-dnh"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],

    "contributes": {
        "languages": [
            {
                "id": "danmakufu",
                "configuration": "./language-configuration.json",
                "extensions": [ ".dnh" ],
                "aliases": [ "dnh", "danmakufu", "Danmakufu" ]
            }
        ],
        "grammars": [
            {
                "language": "id",
                "scopeName": "source.id",
                "path": "./syntaxes/id.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "id",
                "path": "./snippets/id.json"
            }
        ]
    }
}
